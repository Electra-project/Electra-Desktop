{
  "name": "electra-desktop",
  "productName": "Electra Desktop",
  "companyName": "Electra Project",
  "version": "0.0.0",
  "description": "Cross-platform Desktop Wallet for Electra blockchain.",
  "license": "MIT",
  "private": true,
  "main": "./build/main.js",
  "scripts": {
    "build:prod": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.static.js",
    "build:stag": "cross-env NODE_ENV=staging webpack --config ./webpack/webpack.static.js",
    "package:prod": "rimraf ./dist && npm run build:prod && electron-builder",
    "package:stag": "rimraf ./dist && npm run build:stag && electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "start": "cross-env IS_HOT=1 webpack --config ./webpack/webpack.main.hot.js && webpack-dev-server --hot --host 0.0.0.0 --config=./webpack/webpack.renderer.hot.js",
    "start:new": "node ./tasks/removeElectraUserDirectory && npm start",
    "start:prod": "npm run build:prod && electron --noDevServer .",
    "start:stag": "npm run build:stag && electron --noDevServer .",
    "validate": "npm run validate:tsc && npm run validate:lint",
    "validate:lint": "tslint -p src/tsconfig.json",
    "validate:tsc": "tsc -p src/tsconfig.json --noEmit --pretty"
  },
  "build": {
    "productName": "Electra Desktop",
    "copyright": "Electra Project",
    "appId": "org.electraproject.desktop",
    "files": [
      "build"
    ],
    "directories": {
      "output": "dist"
    },
    "linux": {
      "category": "Finance",
      "icon": "build/assets/logos/512x512.png",
      "target": [
        {
          "target": "deb"
        }
      ]
    },
    "mac": {
      "category": "public.app-category.finance"
    },
    "dmg": {
      "icon": "build/assets/logos/logo.icns"
    },
    "win": {
      "icon": "build/assets/logos/logo.ico"
    }
  },
  "dependencies": {
    "ajv": "^6.4.0",
    "await-to-js": "^2.0.1",
    "classnames": "^2.2.5",
    "electra-js": "0.11.2",
    "electron-json-storage": "^4.0.2",
    "lodash": "^4.17.4",
    "material-design-iconic-font": "^2.2.0",
    "moment": "^2.22.0",
    "qrcode.react": "^0.8.0",
    "ramda": "^0.25.0",
    "react": "^16.3.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.3.0",
    "react-hot-loader": "^3.1.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-observable": "^0.18.0",
    "redux-thunk": "^2.2.0",
    "rxjs": "^5.5.8",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@types/bitcoinjs-lib": "^3.3.3",
    "@types/electron-json-storage": "^4.0.0",
    "@types/lodash": "^4.14.106",
    "@types/qrcode.react": "^0.6.3",
    "@types/ramda": "^0.25.21",
    "@types/react-copy-to-clipboard": "^4.2.5",
    "@types/react-dom": "^16.0.4",
    "@types/react-redux": "^5.0.15",
    "@types/react-router-dom": "^4.2.6",
    "@types/redux-logger": "^3.0.5",
    "@types/zxcvbn": "^4.4.0",
    "autoprefixer": "^8.2.0",
    "awesome-typescript-loader": "^4.0.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.7",
    "dotenv-webpack": "^1.5.4",
    "electron": "^1.8.2",
    "electron-builder": "^20.8.2",
    "electron-devtools-installer": "^2.2.3",
    "electron-packager": "^11.2.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.4.3",
    "node-sass": "^4.8.3",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-loader": "^2.1.3",
    "postcss-preset-env": "^3.4.0",
    "prettier": "^1.10.2",
    "redux-devtools": "^3.4.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.2",
    "svg-sprite-loader": "^3.7.3",
    "ts-loader": "^4.0.0",
    "tslint": "^5.9.1",
    "typescript": "2.7.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.9.5",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.6.0"
  },
  "author": {
    "name": "Electra Project",
    "email": "dev@electraproject.org",
    "url": "https://electraproject.org"
  },
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Electra-project/Electra-Desktop.git"
  },
  "bugs": {
    "url": "https://github.com/Electra-project/Electra-Desktop/issues"
  },
  "homepage": "https://electraproject.org"
}
