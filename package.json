{
  "name": "electra-desktop",
  "version": "0.0.0",
  "description": "Cross-platform Desktop Wallet for Electra blockchain.",
  "license": "MIT",
  "private": true,
  "main": "dist/main/index.js",
  "scripts": {
    "build": "webpack --config ./webpack/webpack.prod.js",
    "build:dev:main": "webpack --config ./webpack/webpack.main.dev.js",
    "build:prod": "NODE_ENV=production webpack --config ./webpack/webpack.prod.js && electron --noDevServer .",
    "package": "webpack --config ./webpack/webpack.prod.js",
    "postpackage": "electron-packager ./ --out=./build",
    "start": "npm run build:dev:main && webpack-dev-server --hot --host 0.0.0.0 --config=./webpack/webpack.renderer.dev.js",
    "validate": "npm run validate:tsc && npm run validate:lint",
    "validate:lint": "tslint -p src/tsconfig.json",
    "validate:tsc": "tsc -p src/tsconfig.json --noEmit --pretty"
  },
  "browserslist": [
    "chrome 64"
  ],
  "dependencies": {
    "@types/bitcoinjs-lib": "^3.3.1",
    "@types/express": "^4.11.1",
